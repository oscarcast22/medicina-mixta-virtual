	@-webkit-keyframes dash {
		70% {
			fill-opacity: 0;
		}

		100% {
			fill-opacity: 1;
			stroke-dashoffset: 0;
			stroke-width: 1;
		}
	}

	@keyframes dash {
		70% {
			fill-opacity: 0;
		}

		100% {
			fill-opacity: 1;
			stroke-dashoffset: 0;
			stroke-width: 1;
		}
	}
</style>

<script is:inline>
	document.addEventListener('astro:page-load', function() {
	    const paths = document.querySelectorAll('.medico-path');
	
	    const observerOptions = {
	        root: null,
	        rootMargin: '0px',
	        threshold: 0.18
	    };

	    function handleIntersection(entries, observer) {
	        entries.forEach(entry => {
	            if (entry.isIntersecting) {
					console.log("path observado")
	                entry.target.style.animationName = 'dash';
					entry.target.style.animationDuration = '1.8s';
					entry.target.style.animationTimingFunction = 'ease-in';
					entry.target.style.animationFillMode = 'forwards';
	                observer.unobserve(entry.target);
	            }
	        });
	    }

	    const observer = new IntersectionObserver(handleIntersection, observerOptions);

		if (paths) {
			paths.forEach(path => {
	        	observer.observe(path);
	    	});
		}

	});
</script>
